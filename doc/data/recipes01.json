    "categories": [
    {
        "name": "SALAD"
    }
    ], 
    "ingredients": [
    {
        "sort_number": 1,
        "quantity": 2,
        "name": "patatas",
        "measurement_unit": "unit"
    },
    {
        "sort_number": 2,
        "quantity": 2,
        "name": "tomates",
        "measurement_unit": "unit"
    },
    {
        "sort_number": 3,
        "quantity": 200,
        "name": "judías verdes",
        "measurement_unit": "g"
    },
    {
        "sort_number": 4,
        "quantity": 60,
        "name": "bonito",
        "measurement_unit": "g"
    },
    {
        "sort_number": 5,
        "quantity": 4,
        "name": "huevos de codorniz",
        "measurement_unit": "unit"
    },
    {
        "sort_number": 6,
        "quantity": 0,
        "name": "sal",
        "measurement_unit": "unit"
    },
    {
        "sort_number": 7,
        "quantity": 0,
        "name": "aceite de oliva",
        "measurement_unit": "unit"
    },
    {
        "sort_number": 8,
        "quantity": 0,
        "name": "vinagre de jerez",
        "measurement_unit": "unit"
    }
    ], 
    "directions": [
    {
        "sort_number": 1,
        "description": "Quitar las puntas a las judías verdes, cortarlas longitudinalmente en dos y luego cada trozo resultante en tres de igual tamaño.",
        "image": "",
        "video": "",
        "time": 0
    },
    {
        "sort_number": 2,
        "description": "Cocer las judías en abundante agua con sal durante unos 15 minutos.",
        "image": "",
        "video": "",
        "time": 15
    },
    {
        "sort_number": 3,
        "description": "Cocer las patatas y los huevos en abundante agua con sal. Los huevos deben cocer durante unos 10 minutos y las patatas de 20 a 40 minutos (dependiendo del tamaño).",
        "image": "",
        "video": "",
        "time": 20
    },
    {
        "sort_number": 4,
        "description": "Pelar y trocear las patatas en láminas de unos 2 centímetros.",
        "image": "",
        "video": "",
        "time": 0
    },
    {
        "sort_number": 5,
        "description": "Quitar las cáscaras de los huevos y cortarlos por la mitad a lo largo.",
        "image": "",
        "video": "",
        "time": 0
    },
    {
        "sort_number": 6,
        "description": "Lavar y laminar los tomates muy finalmente (en tiras de medio centímetro aproximadamente).",
        "image": "",
        "video": "",
        "time": 0
    },
    {
        "sort_number": 7,
        "description": "Cuando estén todos los ingredientes a temperatura ambiente, montar el plato poniendo primero una capa de patata, luego tomate, siguiendo con las judías, el bonito y por último los huevos. Aliñar con sal, aceite de oliva y vinagre de Jerez.",
        "image": "",
        "video": "",
        "time": 0
    }
    ]
}

