        "name": "sal",
        "measurement_unit": "unit"
    },
    {
        "sort_number": 12,
        "quantity": 1,
        "name": "pimentón dulce",
        "measurement_unit": "tbsp"
    },
    {
        "sort_number": 13,
        "quantity": 1,
        "name": "orégano",
        "measurement_unit": "tbsp"
    },
    {
        "sort_number": 14,
        "quantity": 0,
        "name": "azafrán",
        "measurement_unit": "unit"
